{
    "ver": "1.0.3",
    "authors": [
        "dylan"
    ],
    "contact": "http://kbase.us/contact-us/",
    "visible": true,
    "categories": ["active","sequence","comparative_genomics"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "desc",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "is_output_name": false
            }
        },
        {
            "id": "input_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseCollections.FeatureSet" ]
            }
        },
        {
            "id": "output_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseTrees.MSA" ],
		"is_output_name": true		  
            }
        },
        {
            "id": "maxiters",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "16" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_integer" : 1
            }
        },
        {
            "id": "maxhours",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.5" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float" : 0.1
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_muscle",
            "method": "MUSCLE_prot",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "desc",
                    "target_property": "desc"
                },
                {
                    "input_parameter": "input_ref",
                    "target_property": "input_ref",
		    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                },
                {
                    "input_parameter": "maxiters",
                    "target_property": "maxiters"
                },
                {
                    "input_parameter": "maxhours",
                    "target_property": "maxhours"
                }
            ],
            "output_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
		    "constant_value": "16",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
